#query.properties
selectAll = SELECT * FROM PRODUCT_STOCK
selectAll2 = SELECT A.IO_NUM ,A.PRODUCT_ID ,B.P_NAME ,A.IO_DATE, A.AMOUNT , A.STATUS FROM PRODUCT_IO A JOIN PRODUCT_STOCK B ON A.PRODUCT_ID = B.PRODUCT_ID
selectone = SELECT A.IO_NUM ,A.PRODUCT_ID ,B.P_NAME ,A.IO_DATE, A.AMOUNT , A.STATUS FROM PRODUCT_IO A JOIN PRODUCT_STOCK B ON A.PRODUCT_ID = B.PRODUCT_ID WHERE A.STATUS='\uC785\uACE0' 