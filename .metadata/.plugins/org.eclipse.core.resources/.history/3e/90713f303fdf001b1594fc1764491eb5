#query.properties
selectAll = SELECT * FROM PRODUCT_STOCK
insertProduct = INSERT INTO PRODUCT_STOCK VALUES (?,?,?,?,?)
updateProduct = UPDATE PRODUCT_STOCK SET PRICE=?, DESCRIPTION=? WHERE PRODUCT_ID=?
deleteProduct = DELETE FROM PRODUCT_STOCK WHERE PRODUCT_ID=?
searchProduct = SELECT * FROM PRODUCT_STOCK WHERE P_NAME=?


IOselectAll = SELECT A.IO_NUM ,A.PRODUCT_ID ,B.P_NAME ,A.IO_DATE, A.AMOUNT , A.STATUS FROM PRODUCT_IO A JOIN PRODUCT_STOCK B ON A.PRODUCT_ID = B.PRODUCT_ID
IOselectInput = SELECT A.IO_NUM ,A.PRODUCT_ID ,B.P_NAME ,A.IO_DATE, A.AMOUNT , A.STATUS FROM PRODUCT_IO A JOIN PRODUCT_STOCK B ON A.PRODUCT_ID = B.PRODUCT_ID WHERE A.STATUS='\uC785\uACE0'
IOselectOutput =  SELECT A.IO_NUM ,A.PRODUCT_ID ,B.P_NAME ,A.IO_DATE, A.AMOUNT , A.STATUS FROM PRODUCT_IO A JOIN PRODUCT_STOCK B ON A.PRODUCT_ID = B.PRODUCT_ID WHERE A.STATUS='\uCD9C\uACE0'